{
  "name": "nobejs",
  "version": "0.1.1-alpha.9",
  "main": "index.js",
  "author": "Rajiv Seelam",
  "license": "MIT",
  "scripts": {
    "nobe:genstory": "node ./core/utils/generateStory",
    "nobe:genuser": "node ./core/utils/randomUser",
    "nobe:version": "node ./core/version",
    "test": "jest --runInBand",
    "test:github": "jest --runInBand",
    "serve": "ENVFILE=.env DEBUG=true nodemon server.js",
    "start": "node server.js",
    "nobe:update": "node ./update && yarn nobe:version",
    "db": "knex",
    "db:migrate": "SET ENVFILE=.env&& knex migrate:latest",
    "db:rollback": "ENVFILE=.env knex migrate:rollback",
    "db:migration": "ENVFILE=.env knex migrate:make",
    "db:seed": "SET ENVFILE=.env&& knex seed:make",
    "db:seedRun": "SET ENVFILE=.env&& knex seed:run",
    "release": "gh release create",
    "bump": "npm version patch && node core/syncVersion.js && git commit -am 'Commit version.md' && git push origin master",
    "bump:minor": "npm version minor",
    "bump:pre": "npm version prerelease --preid=alpha --force"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.3",
    "nodemon": "^2.0.13"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "extract-zip": "^2.0.1",
    "fastify": "^3.21.6",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.1",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.11",
    "njwt": "^1.1.0",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.7.1",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  }
}
